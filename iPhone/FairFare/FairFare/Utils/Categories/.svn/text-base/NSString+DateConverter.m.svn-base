#import "NSString+DateConverter.h"

@implementation NSString (DateConverter)

- (NSString *)convertFormat:(NSString *)format1 toFormat:(NSString *)fomat2{
    DebugLog();
    
    NSDateFormatter *formatter = [[[NSDateFormatter alloc] init] autorelease];
    [formatter setDateFormat:format1];
    NSDate *date = [formatter dateFromString:self];
    
    if (date != nil) {
        [formatter setDateFormat:fomat2];
        NSString *convertedStr  = [formatter stringFromDate:date];
        if (convertedStr != nil) {
            return convertedStr;
        }
    }
    return self;
}

@end
